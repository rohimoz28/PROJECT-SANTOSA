<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View hr_profesion View Tree -->
    <record id="view_hr_profesion_tree" model="ir.ui.view">
        <field name="name">view.hr_profesion.tree</field>
        <field name="model">hr.profesion</field>
        <field name="arch" type="xml">
            <tree>
                <field name="type"/>
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- View hr_profesion form -->
    <record id="view_hr_profesion_form" model="ir.ui.view">
        <field name="name">view.hr_profesion.form</field>
        <field name="model">hr.profesion</field>
        <field name="arch" type="xml">
            <form string="Hr_profesion">
                <sheet>
                    <field name="type" widget="radio" options="{'horizontal': true}"/>
                    <div class="oe_title">
                        <h1>
                            <field name="name" placeholder="Nama..." nolabel="1"/>
                        </h1>
                        <h3>
                            <field name="code" placeholder="Kode..."/>
                        </h3>
                    </div>
                    <group>
                        <group>
                            <field name="area" string="Area"/>
                            <field name="branch_id" string="Unit Bisnis"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_hr_profesion_search" model="ir.ui.view">
        <field name="name">view.hr_profesion.search</field>
        <field name="model">hr.profesion</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Area" name="area" domain="[]" context="{'group_by':'area'}"/>
                    <filter string="Type" name="type" domain="[]" context="{'group_by':'type'}"/>
                    <filter string="Unit Bisnis" name="branch_id" context="{'group_by':'branch_id'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id="action_hr_profesion" model="ir.actions.act_window">
        <field name="name">Profesi</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hr.profesion</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Hr_profesion.
            </p>
        </field>
    </record>
    <!-- <menuitem id="emp_hr_profesion" name="Setting Profesi" sequence='30' parent="hierarchy_id" action="action_hr_profesion"/> -->
<!-- ================================================================================= -->
