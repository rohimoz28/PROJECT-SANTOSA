<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Portal Access Rules -->
    <record id="hr_employee_mutation_rule" model="ir.rule">
        <field name="name">HR Employee Mutation</field>
        <field name="model_id" ref="sanbe_hr_employee_mutation.model_hr_employee_mutations"/>
        <field name="domain_force">
            []
        </field>
        <field name="groups" eval="[
        (4, ref('base.group_system')),
        (4, ref('sanbe_hr_employee_mutation.group_employee_mutation_hrd'))
        ]"/>
        <field name="perm_unlink" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_create" eval="True"/>
    </record>
    

</odoo>
