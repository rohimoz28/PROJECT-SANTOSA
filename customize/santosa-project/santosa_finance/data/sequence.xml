<odoo>
    <data noupdate="1">
        <record id="seq_transaction_no" model="ir.sequence">
            <field name="name">Transaction Number</field>
            <field name="code">account.move.transaction.no</field>
            <field name="padding">6</field>
        </record>
        
        <record id="id_ar_claim" model="account.journal">
            <field name="name">AR Klaim</field>
            <field name="type">general</field>
            <field name="code">ARCLAIM</field> 
            <field name="active">True</field>
            <field name="currency_id" ref="base.IDR"/>
        </record>
        
        <!-- Sequences for purchase.order -->
        <record id="seq_kas_bank_kas" model="ir.sequence">
            <field name="name">Cash Bank (kas)</field>
            <field name="code">cash.bank.kas</field>
            <field name="prefix">CB/Kas/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
        </record>
        
        <!-- Sequences for purchase.order -->
        <record id="seq_kas_bank_bank" model="ir.sequence">
            <field name="name">Cash Bank (Bank)</field>
            <field name="code">cash.bank.bank</field>
            <field name="prefix">CB/Bank/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
