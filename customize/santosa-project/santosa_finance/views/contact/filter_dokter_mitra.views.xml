<odoo>
    <record id="view_partner_filter_inherit" model="ir.ui.view">
        <field name="name">res.partner.filter.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='customer']" position="after">
                <filter string="Dokter Mitra" name="filter_dokter_mitra" domain="[('is_dokter_mitra', '=', True)]"/>
                <filter string="Asuransi" name="filter_asuransi" domain="[('tipe_customer', '=', 'Asuransi')]"/>
            </xpath>
        </field>
    </record>
</odoo>
